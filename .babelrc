{
  "plugins": [
    "transform-class-properties",
    "syntax-decorators",
    "transform-decorators-legacy"
  ],
  "presets": [
    ["env", { 
      "modules": false,
      "targets": {
        "node": "current"
      }
    }],
    "react",
    "stage-0"
  ],
  "env": {
    "development": {
      "presets": [
        "es2017" , "stage-0" , "react"
      ]
    }
  }
}
